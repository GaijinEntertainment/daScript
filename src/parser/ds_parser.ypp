require_module_name
    :   keyword_or_name[name] {
        $$ = $name;
    }
    |   '%' require_module_name[req] {
        *$req = "%" + *$req;
        $$ = $req;
    }
    |   require_module_name[req] '.' keyword_or_name[name] {
        *$req += ".";
        *$req += *$name;
        delete $name;
        $$ = $req;
    }
    |   require_module_name[req] '/' keyword_or_name[name] {
        *$req += "/";
        *$req += *$name;
        delete $name;
        $$ = $req;
