        if ( !yyextra->g_CommentReaders.empty() ) {
            auto tokName = tokAt(scanner,@name);
            for ( auto & crd : yyextra->g_CommentReaders ) {
                crd->afterEnumerationEntry($name->c_str(), tokName);
            }
        }
        delete $name;
        $$ = $pE;
    }
    |   enum_list[pE] NAME[name] '=' expr[value] ';' {
        das_checkName(scanner,*$name,tokAt(scanner,@name));
        if ( !$pE->add(*$name,ExpressionPtr($value),tokAt(scanner,@name)) ) {
            das_yyerror(scanner,"enumeration value already declared " + *$name, tokAt(scanner,@name),
                CompilationError::enumeration_value_already_declared);
        }
        if ( !yyextra->g_CommentReaders.empty() ) {
            auto tokName = tokAt(scanner,@name);
            for ( auto & crd : yyextra->g_CommentReaders ) {
                crd->afterEnumerationEntry($name->c_str(), tokName);
