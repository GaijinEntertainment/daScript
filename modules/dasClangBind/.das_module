options gen2
require fio

[export]
def initialize(project_path : string) {
    if (das_is_dll_build()) {
        register_dynamic_module("{project_path}/dasModuleClangBind.shared_module", "Module_dasClangBind")
    }
    register_native_path("cbind", "cbind_boost", "{project_path}/cbind/cbind_boost.das")
}
